{
    "name": "code-story",
    "displayName": "code story",
    "description": "show statistic about your coding",
    "version": "0.0.1",
    "publisher": "BigMiao",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "Hello World"
            },
            {
                "command": "codeStory.signIn",
                "title": "Sign In"
            },
            {
                "command": "codeStory.signOut",
                "title": "Sign out"
            },
            {
                "command": "codeStory.sync",
                "title": "Sync"
            },
            {
                "command": "codeStory.test.inputbox",
                "title": "input box"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "copy /y src/test/testFile/test node_modules/vscode/bin && npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.21"
    },
    "dependencies": {
        "inversify": "^5.0.1",
        "reflect-metadata": "^0.1.12",
        "typemoq": "^2.1.0"
    }
}
